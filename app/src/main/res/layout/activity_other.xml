<?xml version="1.0" encoding="utf-8"?>
<ScrollView
xmlns:android="http://schemas.android.com/apk/res/android"
android:orientation="horizontal"
android:layout_width="match_parent"
android:layout_height="match_parent"
android:layout_weight="4"
    >

<LinearLayout
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:orientation="vertical"
    android:layout_marginBottom="10dp"


    >




    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="40dp"
        android:layout_gravity="end"
        android:background="@color/colorPrimary"
        android:orientation="vertical"

        >

        <TextView
            android:layout_width="match_parent"
            android:layout_height="20dp"
            android:id="@+id/text_agencyCodeOther"
            android:gravity="center_vertical"
            android:textSize="15dp"
            android:layout_weight="1"
            android:textColor="@color/colorWhite"

            />

        <TextView
            android:layout_width="250dp"
            android:layout_height="15dp"
            android:id="@+id/text_UserNameOther"
            android:layout_weight="1"
            android:textSize="10dp"
            android:gravity="end"
            android:layout_gravity="end"
            android:textColor="@color/colorWhite"
            />

    </LinearLayout>


    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="20dp"
        android:padding="5dp"
        >

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="35dp"
            android:text="Front Left Cornor"
            android:textStyle="bold"
            android:gravity="center_vertical"
            android:textSize="16dp"
            android:id="@+id/Other_fr_lf_cr"
            android:layout_weight="4"

            />

        <ImageButton
            android:layout_width="40dp"
            android:layout_height="35dp"
            android:id="@+id/getGpsOther_frLft"
            android:background="@mipmap/gpsnew"

            />




    </LinearLayout>


    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="5dp"
        android:orientation="horizontal"
        android:visibility="visible"

        >

        <android.support.design.widget.TextInputLayout
            android:id="@+id/input_layout_name_Landfrlft"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textColorHint="@color/colorGray"
            android:layout_weight="4"
            android:visibility="invisible"

            >

            <EditText
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/edt_Other_frLeftLattitude"
                android:hint="@string/hint_Banglow1"
                android:padding="5dp"
                android:visibility="invisible"

                />


        </android.support.design.widget.TextInputLayout>




    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="5dp"
        android:orientation="vertical"
        >

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:visibility="visible"

            >

            <android.support.design.widget.TextInputLayout
                android:id="@+id/input_layout_namefrlft"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:textColorHint="@color/colorGray"
                android:layout_weight="4"
                android:visibility="invisible"
                >

                <EditText
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:id="@+id/edt_Other_frLeftLongitude"
                    android:hint="@string/hint_Banglow2"
                    android:padding="10dp"
                    android:visibility="invisible"

                    />


            </android.support.design.widget.TextInputLayout>

            <android.support.design.widget.TextInputLayout
                android:id="@+id/input_layout_name3lrft"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:textColorHint="@color/colorGray"
                android:layout_weight="4"
                android:visibility="invisible"
                >

                <EditText
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:id="@+id/edt_Other_frLeftAltitude"
                    android:hint="@string/hint_Banglow3"
                    android:padding="10dp"
                    android:visibility="invisible"

                    />


            </android.support.design.widget.TextInputLayout>


        </LinearLayout>



    </LinearLayout>




    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        android:layout_weight="4"
        android:layout_marginTop="5dp"
        >


        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:padding="5dp"

            >

        <TextView
            android:layout_width="match_parent"
            android:layout_height="35dp"
            android:text="Front Right Cornor"
            android:textStyle="bold"
            android:gravity="center_vertical"
            android:textSize="16dp"
            android:layout_weight="4"
            android:id="@+id/Other_fr_rg_cr"
            />

            <ImageButton
                android:layout_width="40dp"
                android:layout_height="35dp"
                android:id="@+id/getGpsOther_frRgt"
                android:background="@mipmap/gpsnew"
                />

        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="5dp"
            android:orientation="horizontal"
            android:visibility="visible"

            >

            <android.support.design.widget.TextInputLayout
                android:id="@+id/input_layout_name1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textColorHint="@color/colorGray"
                android:layout_weight="4"
                android:visibility="invisible"

                >

                <EditText
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:id="@+id/edt_Other_frRgttLattitude"
                    android:hint="@string/hint_Banglow1"
                    android:padding="5dp"
                    android:visibility="invisible"

                    />


            </android.support.design.widget.TextInputLayout>




        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="5dp"
            android:layout_weight="4"
            android:orientation="vertical"
            >

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:visibility="visible"

                >

                <android.support.design.widget.TextInputLayout
                    android:id="@+id/input_layout_name2"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:textColorHint="@color/colorGray"
                    android:layout_weight="4"
                    android:visibility="invisible"
                    >

                    <EditText
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:id="@+id/edt_Other_frRgttLongitude"
                        android:hint="@string/hint_Banglow2"
                        android:padding="10dp"
                        android:visibility="invisible"

                        />


                </android.support.design.widget.TextInputLayout>

                <android.support.design.widget.TextInputLayout
                    android:id="@+id/input_layout_name3"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:textColorHint="@color/colorGray"
                    android:layout_weight="4"
                    android:visibility="invisible"
                    >

                    <EditText
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:id="@+id/edt_Other_frRgttAltitude"
                        android:hint="@string/hint_Banglow3"
                        android:padding="10dp"
                        android:visibility="invisible"

                        />


                </android.support.design.widget.TextInputLayout>


            </LinearLayout>

        </LinearLayout>

    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        android:layout_weight="4"
        android:layout_marginTop="5dp"
        >


        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:padding="5dp"

            >

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="35dp"
            android:text="Back Left Cornor"
            android:textStyle="bold"
            android:textSize="16dp"
            android:gravity="center_vertical"
            android:layout_weight="4"
            android:id="@+id/Other_Bk_lf_cr"
            />

            <ImageButton
                android:layout_width="40dp"
                android:layout_height="35dp"
                android:id="@+id/getGpsOther_BkLfcr"
                android:background="@mipmap/gpsnew"
                />

        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:visibility="visible"

            >

            <android.support.design.widget.TextInputLayout
                android:id="@+id/input_layout_LandBkLeft"
                android:layout_width="wrap_content"
                android:layout_height="5dp"
                android:textColorHint="@color/colorGray"
                android:layout_weight="4"
                android:visibility="invisible"

                >

                <EditText
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:id="@+id/edt_Other_BkLftLattitude"
                    android:hint="@string/hint_Banglow1"
                    android:padding="5dp"
                    android:visibility="invisible"



                    />


            </android.support.design.widget.TextInputLayout>








        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="5dp"
            android:layout_weight="4"
            android:orientation="vertical"
            >

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:visibility="visible"

                >

                <android.support.design.widget.TextInputLayout
                    android:id="@+id/input_layout_name2_LandBkLeft"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:textColorHint="@color/colorGray"
                    android:layout_weight="4"
                    android:visibility="invisible"
                    >

                    <EditText
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:id="@+id/edt_Other_BkLftLongitude"
                        android:hint="@string/hint_Banglow2"
                        android:padding="10dp"
                        android:visibility="invisible"

                        />


                </android.support.design.widget.TextInputLayout>

                <android.support.design.widget.TextInputLayout
                    android:id="@+id/input_layout_name3_LandBkLeft"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:textColorHint="@color/colorGray"
                    android:layout_weight="4"
                    android:visibility="invisible"
                    >

                    <EditText
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:id="@+id/edt_Other_BkLftAltitude"
                        android:hint="@string/hint_Banglow3"
                        android:padding="10dp"
                        android:visibility="invisible"

                        />


                </android.support.design.widget.TextInputLayout>


            </LinearLayout>

        </LinearLayout>


    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        android:layout_weight="4"
        android:layout_marginTop="5dp"
        >

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:padding="5dp"

            >

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="35dp"
            android:text="Back Right Cornor"
            android:textStyle="bold"
            android:textSize="16dp"
            android:gravity="center_vertical"
            android:layout_weight="4"
            android:id="@+id/Other_Bk_Ri_cr"
            />

            <ImageButton
                android:layout_width="40dp"
                android:layout_height="35dp"
                android:id="@+id/getGpsOther_BkRgtcr"
                android:background="@mipmap/gpsnew"
                />
          </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:visibility="visible"

            >

            <android.support.design.widget.TextInputLayout
                android:id="@+id/input_layout_LandBkRight"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textColorHint="@color/colorGray"
                android:layout_weight="4"
                android:visibility="invisible"

                >

                <EditText
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:id="@+id/edt_Other_BkRgtLattitude"
                    android:hint="@string/hint_Banglow1"
                    android:padding="5dp"
                    android:visibility="invisible"

                    />


            </android.support.design.widget.TextInputLayout>








        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="5dp"
            android:layout_weight="4"
            android:orientation="vertical"
            >

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:visibility="visible"

                >

                <android.support.design.widget.TextInputLayout
                    android:id="@+id/input_layout_name2_LandBkRight"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:textColorHint="@color/colorGray"
                    android:layout_weight="4"
                    android:visibility="invisible"
                    >

                    <EditText
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:id="@+id/edt_Other_BkRgtLongitude"
                        android:hint="@string/hint_Banglow2"
                        android:padding="10dp"
                        android:visibility="invisible"

                        />


                </android.support.design.widget.TextInputLayout>

                <android.support.design.widget.TextInputLayout
                    android:id="@+id/input_layout_name3_LandBkRight"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:textColorHint="@color/colorGray"
                    android:layout_weight="4"
                    android:visibility="invisible"
                    >

                    <EditText
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:id="@+id/edt_Other_BkRgtAltitude"
                        android:hint="@string/hint_Banglow3"
                        android:padding="10dp"
                        android:visibility="invisible"

                        />


                </android.support.design.widget.TextInputLayout>


            </LinearLayout>

        </LinearLayout>

    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:padding="7dp"
        >

        <Button
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:id="@+id/btn_Other_sbmit"
            android:background="@color/colorPrimary"
            android:text="@string/btn_banglowSubmit"
            android:padding="10dp"
            android:textSize="18dp"
            android:textColor="@color/colorWhite"
            />


    </LinearLayout>





</LinearLayout>

</ScrollView>